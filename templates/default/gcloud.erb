WRAPPER_SCRIPT_DIR="$( cd -P "$( dirname "$0" )" && pwd -P )"

LIB_DIR="$( dirname "${WRAPPER_SCRIPT_DIR}" )/lib"
if [ -z "$PYTHONPATH" ]; then
  export PYTHONPATH=${LIB_DIR}
else
  export PYTHONPATH=${LIB_DIR}:${PYTHONPATH}
fi

if [ -z "${CLOUDSDK_PYTHON}" ]; then
  CLOUDSDK_PYTHON="<%= @python %>"
fi

__cloudsdk_sitepackages=-S
if [ ${CLOUDSDK_PYTHON_SITEPACKAGES} ]; then
  __cloudsdk_sitepackages=
fi

${CLOUDSDK_PYTHON} ${CLOUDSDK_PYTHON_ARGS-$__cloudsdk_sitepackages} ${WRAPPER_SCRIPT_DIR}/../lib/google/cloud/sdk/gcloud/gcloud.py "$@"
exit_code=$?
if [ $exit_code -ne 0 ]; then exit $exit_code; fi

